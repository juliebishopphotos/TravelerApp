@model IEnumerable<TravelerApp.Models.EatListItem>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Add To Trip", "Create", "TripEat") |
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EatId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EatId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.EatId }) |
            @Html.ActionLink("Details", "Details", new { id = item.EatId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.EatId })
        </td>
    </tr>
}

</table>
